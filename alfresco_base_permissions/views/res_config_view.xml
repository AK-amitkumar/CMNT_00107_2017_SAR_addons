<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_general_configuration_form_inherit_alfresco_permission_service" model="ir.ui.view">
            <field name="name">base.config.settings.form.inherit.alfresco</field>
            <field name="model">base.config.settings</field>
            <field name="inherit_id" ref="base_setup.view_general_configuration"/>
        <field name="arch" type="xml">
                <xpath expr="//group[@name='google_analytics']" position="after">
                    <group string="Alfresco Permission Service">
                        <div>
                            <field name="alfresco_permission_service"/>
                        </div>
                    </group>
                 </xpath>
            </field>
        </record>
    </data>
</odoo>
